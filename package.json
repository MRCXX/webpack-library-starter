{
  "name": "webpack-library-starter",
  "version": "1.1.0",
  "description": "base on webpack-library-starter and add eslint standard by mrc",
  "main": "lib/webpack-library-starter.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require @babel/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require @babel/register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require @babel/register --colors test/*.js",
    "repl": "node -i -e \"$(< ./lib/index.js)\"",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint ./src/*.js --fix",
    "test:lint": "eslint ./test/*.js --fix"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mrcxx/webpack-library-starter.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs",
    "eslint"
  ],
  "author": "MRC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrcxx/webpack-library-starter/issues"
  },
  "homepage": "https://github.com/mrcxx/webpack-library-starter",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "nyc": "^13.1.0",
    "pre-commit": "^1.2.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "chevrotain": "^6.5.0",
    "dayjs": "^1.8.17",
    "lodash": "^4.17.15"
  }
}
